	},
	"portal": {
		"base_url": "https://not-a-real-host.test/#!/",
		"email_from": "no-reply@traffic-portal.test",
		"pass_reset_path": "user",
		"user_register_path": "user"
	},
	"secrets": [
		"blahblah"
	],
	"inactivity_timeout": 60,
	"smtp": {
		"enabled": true,
		"user": "",
		"password": "",
		"address": "172.17.0.1:25"
	},
	"InfluxEnabled": false

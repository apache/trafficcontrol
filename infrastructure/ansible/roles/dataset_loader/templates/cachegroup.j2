{
{% if item.fallbackToClosest is defined and item.fallbackToClosest is not none %}
  "fallbackToClosest": {{ item.fallbackToClosest }},
{% endif %}
{% if item.latitude is defined and item.latitude is not none %}
  "latitude": {{ item.latitude }},
{% endif %}
{% if item.localizationMethods is defined and item.localizationMethods is not none %}
  "localizationMethods": ["{{ item.localizationMethods | join('","')}}"],
{% endif %}
{% if item.longitude is defined and item.longitude is not none %}
  "longitude": {{ item.longitude }},
{% endif %}
  "name": "{{ item.name }}",
{% if parentCachegroupId is defined and parentCachegroupId is not none and parentCachegroupId|length > 0 %}
  "parentCachegroupId": {{ parentCachegroupId }},
{% endif %}
{% if secondaryparentCachegroupId is defined and secondaryparentCachegroupId is not none and secondaryparentCachegroupId|length > 0%}
  "secondaryParentCachegroupId": {{ secondaryparentCachegroupId }},
{% endif %}
  "shortName": "{{ item.shortName }}",
  "typeId": {{ get_all_types.json | json_query(type_query) }}
}

{
	"name": "traffic-portal-angular8",
	"version": "0.1.0",
	"scripts": {
		"ng": "ng",
		"start": "ng serve",
		"build": "ng build",
		"test": "ng test",
		"lint": "ng lint",
		"e2e": "ng e2e",
		"compile:server": "webpack --config webpack.server.config.js --progress --colors",

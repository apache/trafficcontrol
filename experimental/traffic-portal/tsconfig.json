{
	"compileOnSave": false,
	"compilerOptions": {
		"allowUnreachableCode": false,
		"allowUnusedLabels": false,
		"baseUrl": "./",
		"declaration": false,
		"downlevelIteration": true,
		"exactOptionalPropertyTypes": true,
		"experimentalDecorators": true,
		"importHelpers": true,
		"emitDecoratorMetadata": true,
		"module": "es2020",
		"outDir": "./dist/out-tsc",
		"strict": true,
		"target": "es2017",
		"lib": [
			"es2020",
			"dom"
		]
	}
}
